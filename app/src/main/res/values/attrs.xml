<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="text" format="string" />
    <attr name="title" format="string" />

    <declare-styleable name="TraktActionView">
        <attr name="colorChecked" format="color" />
        <attr name="colorPressed" format="color" />
    </declare-styleable>

    <declare-styleable name="ReadMoreView">
        <attr name="content" format="reference" />
        <attr name="text" />
    </declare-styleable>

    <declare-styleable name="TraktImageView">
        <attr name="type" format="string" />
    </declare-styleable>

    <declare-styleable name="RoundView">
        <attr name="android:color"/>
    </declare-styleable>

    <declare-styleable name="BezelImageView">
        <attr name="maskDrawable" format="reference" />
        <attr name="borderDrawable" format="reference" />
        <attr name="desaturateOnPress" format="boolean" />
    </declare-styleable>
</resources>